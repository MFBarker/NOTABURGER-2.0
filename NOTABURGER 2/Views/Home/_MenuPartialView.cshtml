            <tr> <td>Calories: @Model.nutTable_solo[0]</td>          <td>Calories from Fat: @Model.nutTable_solo[1]</td>  <td>Saturated Fat (g): @Model.nutTable_solo[2]</td> </tr>
            <tr> <td>Trans Fat (g): @Model.nutTable_solo[3]</td>     <td>Cholesterol (g): @Model.nutTable_solo[4]</td>    <td>Sodium (mg): @Model.nutTable_solo[5]</td> </tr>
            <tr> <td>Dietary Fiber (g): @Model.nutTable_solo[6]</td> <td>Protein (g): @Model.nutTable_solo[7]</td>        <td>Sugars (g): @Model.nutTable_solo[8]</td> </tr>
            </table>*@

                    <b>Meal:</b>
                    @*<table class="nutTable">
            <tr> <td>Calories: @Model.nutTable_meal[0]</td>           <td>Calories from Fat: @Model.nutTable_meal[1]</td>  <td>Saturated Fat (g): @Model.nutTable_meal[2]</td> </tr>
