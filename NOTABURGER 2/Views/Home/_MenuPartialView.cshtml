                <tr>
                    <td>
                        <input type="checkbox" name="addOns" value="Chili" />
                        <label for="Chili">Chili</label>
                    </td>
                    <td>
                        <input id="Bacon" type="checkbox" name="addOns" value="Bacon Slices" onclick="check_AddOns()" />
                        <label for="Bacon Slices">Bacon Slices</label>
                        <input type="number" id="BCNquantity" name="quantity" min="1" max="8" class="quantity">
                    </td>
                    <td>
                        <input id="Jalapeno" type="checkbox" name="addOns" value="Jalapenos" onclick="check_AddOns()" />
                        <label for="Jalapenos">Jalapenos</label>
                        <input type="number" id="JPNquantity" name="quantity" min="1" max="8" class="quantity">
                    </td>
                    <td>
                        <input id="GrJalapenos" type="checkbox" name="addOns" value="Grilled Jalapenos" onclick="check_AddOns()" />
                        <label for="Grilled Jalapenos">Grilled Jalapenos</label>
                        <input type="number" id="GrJPNquantity" name="quantity" min="1" max="8" class="quantity">
                    </td>
                    <td>
                        <input id="Avocado" type="checkbox" name="addOns" value="TB_Avocado" onclick="check_AddOns()" />
                        <label for="Avocado">Avocados</label>
                        <input type="number" id="AVquantity" name="quantity" min="1" max="8" class="quantity">
                    </td>
                    <td>
                        <input id="Chiles" type="checkbox" name="addOns" value="Green TB_Chiles" onclick="check_AddOns()" />
                        <label for="Green TB_Chiles">Green Chiles</label>
                        <input type="number" id="GCquantity" name="quantity" min="1" max="8" class="quantity">
                    </td>
                </tr>
            </table>
            <!-- Toppings -->
            <div><b>Toppings</b></div>
            <table>
                <tr>
                    <td>
                        <input id="TomatoS" type="checkbox" name="toppings" value="Tomato" onclick="check_Toppings()" />
                        <label for="Tomato">Tomato</label>
                        <select name="selection" id="Tomato">
                            <option value="Less">Less</option>
                            <option value="Regular">Regular</option>
                            <option value="More">More</option>
                        </select>
                    </td>
                    <td>
                        <input id="LettuceS" type="checkbox" name="toppings" value="Lettuce" onclick="check_Toppings()" />
                        <label for="Lettuce">Lettuce</label>
                        <select name="selection" id="Lettuce">
                            <option value="Less">Less</option>
                            <option value="Regular">Regular</option>
                            <option value="More">More</option>
                        </select>
                    </td>
                    <td>
                        <input id="pickles" type="checkbox" name="toppings" value="Pickles" onclick="check_Toppings()" />
                        <label for="Pickles">Pickles</label>
                        <select name="selection" id="Pickles">
                            <option value="Less">Less</option>
                            <option value="Regular">Regular</option>
                            <option value="More">More</option>
                        </select>
                    </td>
                </tr>
                <tr>
                    <td>
                        <input id="onionD" type="checkbox" name="toppings" value="Diced Onions" onclick="check_Toppings()" />
