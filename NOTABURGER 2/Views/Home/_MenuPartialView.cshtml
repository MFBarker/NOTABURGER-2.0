                        <label for="Frisée">Frisée</label>
                        <select name="selection" id="Frisee">
                            <option value="Less">Less</option>
                            <option value="Regular">Regular</option>
                            <option value="More">More</option>
                        </select>
                    </td>
                </tr>
            </table>
            <!-- Sauces -->
            <div><b>Sauces</b></div>
            <table>
                <tr>
                    <td>
                        <input id="mustard" type="checkbox" name="sauces" value="Mustard" onclick="check_Sauce()" />
                        <label for="Mustard">Mustard</label>
                        <select name="selection" id="Mustard">
                            <option value="Less">Less</option>
                            <option value="Regular">Regular</option>
                            <option value="More">More</option>
                        </select>
                    </td>
                    <td>
                        <input id="Mayonnaise" type="checkbox" name="sauces" value="Mayonnaise" onclick="check_Sauce()" />
                        <label for="Mayonnaise">Mayonnaise</label>
                        <select name="selection" id="Mayo">
                            <option value="Less">Less</option>
                            <option value="Regular">Regular</option>
                            <option value="More">More</option>
                        </select>
                    </td>
                    <td>
                        <input id="TB_KetchupS" type="checkbox" name="sauces" value="Ketchup" onclick="check_Sauce()" />
                        <label for="Ketchup">Ketchup</label>
                        <select name="selection" id="Ketchup">
                            <option value="Less">Less</option>
                            <option value="Regular">Regular</option>
                            <option value="More">More</option>
                        </select>
                    </td>
                    <td>
                        <input id="HoneyBBQ" type="checkbox" name="sauces" value="Honey BBQ" onclick="check_Sauce()" />
                        <label for="Honey BBQ">Honey BBQ</label>
                        <select name="selection" id="BBQ">
                            <option value="Less">Less</option>
                            <option value="Regular">Regular</option>
                            <option value="More">More</option>
                        </select>
                    </td>
                </tr>
            </table>
            <br />
            @if (Model.name == "GrassBurger")
            {
                <input type="image" src="Resources/btnAddToOrder.png" class="AddButton center" onclick="addClick()" />
            }
            else
            {
                <input type="image" src="Resources/btnAddToOrder.png" class="AddButton center" onclick="dudClick()" />
            }
        </div>
        <div class="itemInfo">
            <b>Contains:</b> @Model.allergens <br />
            <b>Nutritional Facts:</b>
            @if (Model.nutTable_solo != null && Model.nutTable_meal != null)
            {
                <div class="shrinkText">
                    <b>Sandwich Only:</b>
