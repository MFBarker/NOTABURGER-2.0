@if (Model.type == "burger")
{
    <h2><b>@Model.name</b></h2>
    <br />
    <input type="image" src="@Model.imageSrc" class="collapsible item center" padding="0px" margin="0px" />
    <div class="itemInfo">
        <p>
            <b>Sandwich Only:</b> $@Model.soloPrice (@Model.soloCal Cal) <br />
            <b>NotaMeal®:</b> $@Model.mealPrice (@Model.mealCal Cal) <br /> <br />
            <b>Includes:</b> @Model.includes
        </p>
        <div class="itemInfo">
            <!-- Bread -->
            <div><b>Bread</b></div>
            <table>
                <tr>
                    <td>
                        <input id="LargeBun" type="radio" name="bread" value="Large Bun">
                        <label for="Large Bun">Large Bun</label>
                    </td>
                    <td>
                        <input type="radio" name="bread" value="Small Bun">
                        <label for="Small Bun">Small Bun</label>
                    </td>
                    <td>
                        <input type="radio" name="bread" value="Brioche Bun">
                        <label for="Brioche Bun">Brioche Bun</label>
                    </td>
                    <td>
                        <input type="radio" name="bread" value="Texas Toast">
                        <label for="Texas Toast">Texas Toast</label>
                    </td>
                    <td>
                        <input type="radio" name="bread" value="No Bun">
                        <label for="No Bun">No Bun</label>
                    </td>
                </tr>
            </table>
            <!-- Extra Patty -->
            <div><b>Extra Patty</b></div>
            <table>
                <tr>
                    <td>
                        <input id="patty" type="radio" name="extraPatty" value="Large @Model.pattyType Patty (1)" />
                        <label for="Large @Model.pattyType Patty">Large @Model.pattyType Patty</label>
                        <input type="number" id="quantity" name="quantity" min="1" max="8" value="1" class="quantity">
                    </td>
                </tr>
            </table>
            <!-- Cheese -->
            <div><b>Cheese</b></div>
            <table>
                <tr>
                    <td>
                        <input id="AmCheese" type="checkbox" name="cheese" value="American Cheese" onclick="check_AddOns()" />
